{
  "$schema": "https://raw.githubusercontent.com/AngelMunoz/Perla/v1.0.0-beta-011/perla.schema.json",
  "index": "./index.html",
  "mountDirectories": {
    "/src": "./src"
  },
  "fable": {
    "project": "./src/OnlineTool.fsproj",
    "extension": ".js"
  },
  "devServer": {
    "port": 8900,
    "liveReload": true,
    "useSSL": true
  },
  "build": {
    "includes": [
      "src/*.ico",
      "src/*.png"
    ]
  },
  "esbuild": {
    "target": "es2020"
  },
  "envPath": "/telplin/env.js",
  "dependencies": [
    {
      "name": "@monaco-editor/react",
      "version": "4.4.6"
    },
    {
      "name": "lz-string",
      "version": "1.5.0"
    },
    {
      "name": "react",
      "version": "18.2.0"
    },
    {
      "name": "react-dom",
      "version": "18.2.0"
    },
    {
      "name": "use-sync-external-store/shim",
      "version": "1.2.0"
    }
  ],
  "devDependencies": []
}